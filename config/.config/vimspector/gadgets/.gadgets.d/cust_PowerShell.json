{
    "cust_PowerShellEditorServices": {
        "launguage": [ "powershell" ],
        "download": {
            "url": "https://github.com/PowerShell/PowerShellEditorServices/releases/download/v{$version}/${file_name}",
            "format": "zip"
        },
        "all": {
            "version": "3.3.1",
            "file_name": "PowerShellEditorServices.zip",
            "adapters": {
                "cust_PowerShellEditorServices": {
                    "name": "poshdbg",
                    "type": "PowerShell",
                    "command": [
                        "PSES_PATH=${gadgetDir}/cust_PowerShellEditorServices/PowerShellEditorServices",
                        "SESSION_PATH=$(pwd)",
                        "pwsh",
                        "-NoProfile",
                        "-Command",
                        "$PSES_PATH/Start-EditorServices.ps1",
                        "-BundleModulePath",
                        "$PSES_PATH",
                        "-LogPath",
                        "$SESSION_PATH/session.log",
                        "-SessionDetailsPath",
                        "$SESSION_PATH/session.json",
                        "-FeatureFlags",
                        "@()",
                        "-AdditionalModules",
                        "@()",
                        "-HostName",
                        "Vimspector",
                        "-HostProfileId",
                        "vimspector",
                        "-HostVersion",'
                        "'1.0.0'",
                        "-LogLevel",
                        "Verbose",
                        "-DebugServiceOnly"
                    ]
        },

